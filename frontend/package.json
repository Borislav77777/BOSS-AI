{
  "name": "barsukov-platform-ts",
  "version": "1.0.0",
  "description": "BARSUKOV OS - Модульная платформа с TypeScript",
  "type": "module",
  "main": "src/index.tsx",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "preview:prod": "vite preview --port 4173 --mode production",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "deploy": "./deploy.sh --production",
    "deploy:analyze": "./deploy.sh --production --analyze",
    "deploy:safe": "bash deploy-safe.sh",
    "analyze": "npm run build:prod && npx vite-bundle-analyzer dist"
  },
  "dependencies": {
    "@babylonjs/core": "^8.33.2",
    "@babylonjs/gui": "^8.33.2",
    "@babylonjs/loaders": "^8.33.2",
    "@react-spring/web": "^10.0.3",
    "axios": "^1.12.2",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.263.1",
    "mermaid": "^11.12.0",
    "node-telegram-bot-api": "^0.66.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-spring": "^10.0.3",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@emotion/is-prop-valid": "^1.4.0",
    "@sentry/react": "^10.19.0",
    "@sentry/types": "^10.19.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^24.6.0",
    "@types/node-telegram-bot-api": "^0.64.11",
    "@types/react": "^18.3.25",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/ui": "^0.34.6",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.9.3",
    "vite": "^4.5.14",
    "vitest": "^0.34.6"
  },
  "keywords": [
    "barsukov",
    "platform",
    "typescript",
    "react",
    "modular",
    "ai"
  ],
  "author": "BARSUKOV OS Team",
  "license": "MIT"
}
