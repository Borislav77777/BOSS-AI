# Production Environment Variables для BARSUKOV OS
# Домен: ballu-splitsistema.ru

# Основные настройки приложения
VITE_APP_TITLE=BARSUKOV OS
VITE_APP_VERSION=3.3.0
VITE_APP_ENV=production

# API настройки
VITE_API_URL=https://boss-ai.online/api
VITE_API_BASE_URL=https://boss-ai.online

# Домен и URL
VITE_DOMAIN=boss-ai.online
VITE_BASE_URL=https://boss-ai.online

# Настройки для production
VITE_NODE_ENV=production
VITE_DEBUG=false
VITE_ANALYTICS=true

# Настройки безопасности
VITE_CSP_ENABLED=true
VITE_HTTPS_ONLY=true

# Настройки кэширования
VITE_CACHE_VERSION=1.0.0
VITE_SW_ENABLED=true

# Настройки мониторинга
VITE_SENTRY_DSN=
VITE_GA_TRACKING_ID=
VITE_YANDEX_METRIKA_ID=

# Настройки для AI сервисов (если используются)
VITE_OPENAI_API_KEY=
VITE_ANTHROPIC_API_KEY=

# Настройки для внешних сервисов
VITE_STRIPE_PUBLIC_KEY=
VITE_PAYPAL_CLIENT_ID=

# Настройки для социальных сетей
VITE_VK_APP_ID=
VITE_GOOGLE_CLIENT_ID=

# Настройки для уведомлений
VITE_PUSH_PUBLIC_KEY=
VITE_FCM_SERVER_KEY=

# Настройки для файлов
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,image/webp,application/pdf,text/plain

# Настройки для чата
VITE_CHAT_HISTORY_LIMIT=100
VITE_CHAT_AUTO_SAVE=true

# Настройки для рабочего пространства
VITE_WORKSPACE_AUTO_SAVE=true
VITE_WORKSPACE_BACKUP_INTERVAL=300000

# Настройки для тем
VITE_DEFAULT_THEME=dark
VITE_THEME_PERSISTENCE=true

# Настройки для производительности
VITE_LAZY_LOADING=true
VITE_IMAGE_OPTIMIZATION=true
